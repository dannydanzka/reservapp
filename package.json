{
  "name": "ReservApp",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "react-native run-android",
    "android:debug": "react-native run-android --mode debug",
    "android:release": "react-native run-android --mode release",
    "ios": "react-native run-ios",
    "ios:debug": "react-native run-ios --configuration Debug",
    "ios:release": "react-native run-ios --configuration Release",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "build": "yarn build:android:release && yarn build:ios:release",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "build:ios:debug": "xcodebuild -workspace ios/ReservApp.xcworkspace -scheme ReservApp -configuration Debug -destination generic/platform=iOS -archivePath ios/build/ReservApp.xcarchive archive",
    "build:ios:release": "xcodebuild -workspace ios/ReservApp.xcworkspace -scheme ReservApp -configuration Release -destination generic/platform=iOS -archivePath ios/build/ReservApp.xcarchive archive",
    "build:ios:ipa": "xcodebuild -exportArchive -archivePath ios/build/ReservApp.xcarchive -exportPath ios/build -exportOptionsPlist ios/ExportOptions.plist",
    "lint": "yarn lint:js && yarn lint:styles && yarn type-check",
    "lint:js": "eslint . --config eslint.config.js --cache",
    "lint:styles": "stylelint \"src/**/*.styled.{ts,tsx}\" --cache",
    "lint:format": "prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --check",
    "lint:fix": "yarn lint:js --fix; yarn lint:styles --fix; yarn format",
    "lint:fix-safe": "yarn lint:js --fix --max-warnings 0 && yarn lint:styles --fix --max-warnings 0 && yarn format",
    "lint:fix-all": "yarn lint:fix && yarn type-check",
    "lint:errors": "yarn lint:js --quiet && yarn lint:styles --quiet && yarn type-check",
    "lint:errors:js": "eslint . --config eslint.config.js --cache --quiet",
    "lint:errors:styles": "stylelint \"src/**/*.styled.{ts,tsx}\" --cache --quiet",
    "lint:staged": "eslint --cache --fix",
    "lint:staged:styles": "stylelint --cache --fix",
    "lint:performance": "TIMING=1 yarn lint:js",
    "lint:hooks": "eslint 'src/libs/presentation/hooks/**/*.{ts,tsx}' --cache --fix",
    "lint:components": "eslint 'src/libs/presentation/components/**/*.{ts,tsx}' --cache --fix",
    "lint:services": "eslint 'src/libs/infrastructure/services/**/*.{ts,tsx}' --cache --fix",
    "lint:screens": "eslint 'src/libs/presentation/screens/**/*.{ts,tsx}' --cache --fix",
    "lint:modules": "eslint 'src/modules/**/*.{ts,tsx}' --cache --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,md,json}\" --ignore-path .gitignore",
    "prettier": "prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --check",
    "prettier:fix": "prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --write",
    "stylelint": "stylelint \"src/**/*.styled.{ts,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.styled.{ts,tsx}\" --fix",
    "analyze:exports": "grep -r 'export default' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "analyze:console": "grep -r 'console\\.' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "analyze:unused": "yarn lint:js --rule 'import/no-unused-modules: error'",
    "analyze:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --watch",
    "pre-commit": "yarn lint:errors && yarn format:check",
    "pre-push": "yarn lint",
    "clean": "yarn clean:cache && yarn clean:builds && yarn clean:modules",
    "clean:cache": "react-native start --reset-cache",
    "clean:builds": "rm -rf android/app/build && rm -rf ios/build && rm -rf ios/DerivedData",
    "clean:modules": "rm -rf node_modules && yarn install",
    "clean:ios": "cd ios && xcodebuild clean && rm -rf build && rm -rf DerivedData",
    "clean:android": "cd android && ./gradlew clean",
    "clean:all": "yarn clean && yarn clean:ios && yarn clean:android",
    "pods": "cd ios && npx pod-install",
    "pods:update": "cd ios && pod update",
    "pods:repo-update": "cd ios && pod repo update",
    "fastlane:ios:beta": "cd ios && bundle exec fastlane beta",
    "fastlane:ios:release": "cd ios && bundle exec fastlane release",
    "fastlane:android:beta": "cd android && bundle exec fastlane beta",
    "fastlane:android:release": "cd android && bundle exec fastlane release",
    "fastlane:setup": "cd ios && bundle install && cd ../android && bundle install",
    "deploy:ios:testflight": "yarn fastlane:ios:beta",
    "deploy:ios:appstore": "yarn fastlane:ios:release",
    "deploy:android:playstore:beta": "yarn fastlane:android:beta",
    "deploy:android:playstore:release": "yarn fastlane:android:release",
    "deploy:apk:debug": "yarn build:android:debug && echo 'Debug APK generated at: android/app/build/outputs/apk/debug/app-debug.apk'",
    "deploy:apk:release": "yarn build:android:release && echo 'Release APK generated at: android/app/build/outputs/apk/release/app-release.apk'",
    "deploy:bundle:release": "yarn build:android:bundle && echo 'Release AAB generated at: android/app/build/outputs/bundle/release/app-release.aab'",
    "security:audit": "yarn audit --audit-level moderate",
    "security:fix": "yarn audit --fix",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info",
    "link": "npx react-native link",
    "unlink": "npx react-native unlink",
    "postinstall": "echo 'Running post-install tasks...' && cd ios && npx pod-install && echo 'CocoaPods installation completed successfully'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native/new-app-screen": "0.80.0",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/drawer": "^7.5.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "@reduxjs/toolkit": "^2.8.2",
    "await-to-js": "^3.0.0",
    "axios": "^1.11.0",
    "i18next": "^25.3.2",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react-native": "^0.537.0",
    "querystring": "^0.2.1",
    "react": "19.1.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.80.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.1",
    "react-native-worklets": "^0.4.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react": "^19.1.0",
    "@types/styled-components-react-native": "^5.2.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-ft-flow": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^7.6.3",
    "eslint-restricted-globals": "^0.2.0",
    "globals": "^16.3.0",
    "jest": "^29.6.3",
    "postcss-styled-syntax": "^0.7.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "2.8.8",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^7.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.7.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.39.0"
  },
  "engines": {
    "node": ">=18"
  }
}
